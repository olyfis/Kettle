    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>RAPPORT_DEV</connection>
    <sql>SELECT DISTINCT  
	APP_KEY,
	CO_accountingOwnerCode,
	CASE 
		WHEN RIGHT ( APP_CONTRACT_NUMBER, 11 ) IS NULL THEN 
		CASE 
			WHEN LEN ( CO_UD_CO_CONTRACT_ID ) &lt;&gt; 11 THEN RIGHT ( CO_UD_CO_CONTRACT_ID, 11 ) 
			ELSE CO_UD_CO_CONTRACT_ID 
		END 
		ELSE RIGHT ( APP_CONTRACT_NUMBER, 11 ) 
	END AS APP_CONTRACT_NUMBER,
	CO_custName,
	MAX( CO_UD_CO_APP_STATUS ) AS MaxCo_ud_co_app_status,
	MAX( DISTINCT CHG_DATE_TIME ) AS CHG_DATE_TIME,
	CO_branch,
	CO_programCode,
	CO_programCode_TBDESC,
	CO_productLine,
	CASE 
		WHEN ( CO_equipmentCost - SUM ( DISTINCT ( 
		CASE 
			WHEN AS_model IN ( 'PVFCF', 'PVRESIDUAL', 'UPFRONT TAX', 'BILLED &amp; OUT', 'PREMIUM', 'PROP TAX', 'ETA ROLLOVER', 'RENT ROLLOVER' ) THEN AS_listPrice 
			ELSE 0 
		END ) ) ) IS NULL THEN 0 
		ELSE ( CO_equipmentCost - SUM ( DISTINCT ( 
		CASE 
			WHEN AS_model IN ( 'PVFCF', 'PVRESIDUAL', 'UPFRONT TAX', 'BILLED &amp; OUT', 'PREMIUM', 'PROP TAX', 'ETA ROLLOVER', 'RENT ROLLOVER' ) THEN AS_listPrice 
			ELSE 0 
		END ) ) ) 
	END AS COLUMN0004,
	CASE 
		WHEN ( CO_residualAmount IS NULL ) THEN ( 0 ) 
		ELSE ( CO_residualAmount ) 
	END AS CO_residualAmount,
	MAX( ISNULL( CO_promotion, 0 ) ) AS COLUMN0003,
	CO_UD_CO_INTEGRATION_CHARGES_TBDESC AS CO_UD_CO_INTEGRATION_CHARGES_T,
	CO_UD_CO_OERPRO_INCLUDED_TBDESC AS CO_UD_CO_OERPRO_INCLUDED_TBDES,
	MAX( CO_KEY ) AS MaxCo_key,
	MAX( ShippingComments_KEY ) AS MaxShippingcomments_key,
	ShippingComments_UD_SC_SHIPPING_COMMENT AS ShippingComments_UD_SC_SHIPPIN 
FROM 
	RPT_CO RPT_CO
	LEFT OUTER JOIN APPLICATION APPLICATIO
	ON 
		RPT_CO.APP_FKEY = APPLICATIO.APP_KEY
	LEFT OUTER JOIN CHANGE_HISTORY CHANGE_HIS
	ON 
		CHANGE_HIS.CHG_APP_FKEY = RPT_CO.APP_FKEY
	LEFT OUTER JOIN CONTRACT_SETUP CONTRACT_S
	ON 
		APPLICATIO.APP_KEY = CONTRACT_S.CTS_APP_FKEY
	LEFT OUTER JOIN DECISION_CODE_HISTORY DECISION_C
	ON 
		RPT_CO.APP_FKEY = DECISION_C.APP_FKEY
	LEFT OUTER JOIN RPT_ASSET RPT_ASSET
	ON 
		RPT_CO.CO_KEY = RPT_ASSET.CO_FKEY
	LEFT OUTER JOIN RPT_ShippingComments RPT_SHIPPI
	ON 
		RPT_CO.CO_KEY = RPT_SHIPPI.CO_FKEY
	 
WHERE 
	( CHG_FIELD_NAME = 'UD_CO_APP_STATUS'
	AND ( CHG_NEW_DATA = '01'
	OR CHG_NEW_DATA = '03')
	AND CO_UD_CO_APP_STATUS IS NOT NULL)
	AND CO_programCode &lt;&gt; '0042'
	AND ( CTS_DECISION_CODE_FKEY &lt;&gt; 7
	OR CTS_DECISION_CODE_FKEY IS NULL)
	AND CO_custName &lt;&gt; 'TEST APPLICATIONxyz'
	AND APP_KEY NOT IN( 5656, 9355 ) 

GROUP BY 
	APP_KEY,
	CO_accountingOwnerCode,
	CASE 
	WHEN RIGHT ( APP_CONTRACT_NUMBER, 11 ) IS NULL THEN 
	CASE 
		WHEN LEN ( CO_UD_CO_CONTRACT_ID ) &lt;&gt; 11 THEN RIGHT ( CO_UD_CO_CONTRACT_ID, 11 ) 
		ELSE CO_UD_CO_CONTRACT_ID 
	END 
	ELSE RIGHT ( APP_CONTRACT_NUMBER, 11 ) 
END,
	CO_custName,
	CO_branch,
	CO_programCode,
	CO_programCode_TBDESC,
	CO_productLine,
	CO_equipmentCost,
	CASE 
	WHEN ( CO_residualAmount IS NULL ) THEN ( 0 ) 
	ELSE ( CO_residualAmount ) 
END,
	CO_UD_CO_INTEGRATION_CHARGES_TBDESC,
	CO_UD_CO_OERPRO_INCLUDED_TBDESC,
	ShippingComments_UD_SC_SHIPPING_COMMENT 
HAVING 
	MAX( APPLICATIO.APP_DECISION_CODE ) &lt;&gt; 5 
ORDER BY 
	1</sql>
