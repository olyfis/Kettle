    <sql>SELECT 
	LS_MASTER.ID,
	UATB_SC_CUST_NAME,
	UATB_SC_ACTIV_DATE,
	CONTRACT_TERM,
	TERM_DATE,
	UATB_JS_TOTAL_SERVICE_CONTRACT,
	UATB_JS_CURRENT_SERVICE_RATE * UATB_SC_CPP_TERM_CALC3A AS COLUMN0000,
	UATB_SC_TODAY,
	UATB_JS_EQUIP_CITY,
	UATB_JS_EQUIP_STATE,
	UATB_SC_MRKTNG_REP2_DESC,
	UATB_JS_CCAN_SOLDTO 
FROM 
	LS_MASTER_RPTS_NF LS_MASTER,  
	CS_MASTER_NF CS_MASTER 
WHERE 
	LS_MASTER.CUST_CREDIT_ACCT = CS_MASTER.ID
	AND ( PROMOTION &lt;&gt; '31'
	AND UATB_JS_SERVICE_ON_LEASE = 'YES'
	AND BOOKING_DATE = '2018-08-08')
